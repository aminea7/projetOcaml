digraph finite_state_machine {

	rankdir=LR;

	size="8,5"

	node [shape = circle];

4 -> 5 [label = "4/4" ];
4 -> 3 [label = "7/7" ];
3 -> 5 [label = "15/20" ];
3 -> 2 [label = "4/9" ];
2 -> 4 [label = "11/14" ];
2 -> 1 [label = "4/4" ];
1 -> 3 [label = "12/12" ];
1 -> 2 [label = "7/10" ];
0 -> 2 [label = "4/13" ];
0 -> 1 [label = "15/16" ];
}
